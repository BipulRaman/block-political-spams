{
    "description": "Block Political Spams / fake posts in your facebook newsfeed.",
    "manifest_version": 2,
    "name": "Block Political Spams",
    "author": "Bipul Raman",
    "version": "1.1",
    "homepage_url": "https://github.com/BipulRaman/block-political-spams",
    "icons": {
        "48": "logo.png"
    },
    "permissions": [
        "webRequest",
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://www.facebook.com/*"
            ],
            "js": [
                "app.js"
            ]
        }
    ]
}